<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"

    tools:context="com.example.hamzasaleem.hci_new.CancelOrderActivity">

    <com.makeramen.roundedimageview.RoundedImageView
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/imagesale1"
        android:layout_height="180dp"
        android:layout_width="180dp"
        app:riv_corner_radius="5dip"
        android:src="@drawable/cart"
        app:riv_mutate_background="true"
        app:riv_tile_mode="repeat"
        app:riv_oval="false"


        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <com.makeramen.roundedimageview.RoundedImageView
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/imagesale2"
        android:layout_height="180dp"
        android:layout_width="180dp"
        app:riv_corner_radius="5dip"
        app:riv_border_color="@color/colorPrimary"
        app:riv_border_width="2dp"
        android:src="@drawable/customer"
        app:riv_mutate_background="true"
        app:riv_tile_mode="repeat"
        app:riv_oval="false"

        android:onClick="CancelBtn"

        android:layout_below="@+id/imagesale1"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="36dp"
        android:layout_marginTop="71dp" />

    <com.makeramen.roundedimageview.RoundedImageView
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/imagesale3"
        android:layout_height="100dp"
        android:layout_width="100dp"
        app:riv_corner_radius="5dip"
        android:src="@drawable/back"
        app:riv_mutate_background="true"
        app:riv_tile_mode="repeat"
        app:riv_oval="false"

        android:onClick="BackBtn"

        android:layout_alignBottom="@+id/imagesale2"
        android:layout_toStartOf="@+id/imagesale1"
        android:layout_marginBottom="31dp" />



</RelativeLayout>
